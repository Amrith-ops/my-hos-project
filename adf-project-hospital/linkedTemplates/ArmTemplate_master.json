{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-project-hospital"
		},
		"hosprojectvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://hos-project-keyvault2.vault.azure.net/"
		},
		"FinalProcessedDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost"
		},
		"FinalProcessedDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "hospital_final_dataset"
		},
		"FinalProcessedDatabase_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "root"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-project-hospital_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"hosprojectvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('hosprojectvault_properties_typeProperties_baseUrl')]"
					},
					"FinalProcessedDatabase_properties_typeProperties_server": {
						"value": "[parameters('FinalProcessedDatabase_properties_typeProperties_server')]"
					},
					"FinalProcessedDatabase_properties_typeProperties_database": {
						"value": "[parameters('FinalProcessedDatabase_properties_typeProperties_database')]"
					},
					"FinalProcessedDatabase_properties_typeProperties_username": {
						"value": "[parameters('FinalProcessedDatabase_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}